<flynt-component client:visible name="BlockWysiwyg" class="componentSpacing" {{ options.theme ? 'data-theme="' ~ options.theme ~ '"' }}>
  <div class="container centerMaxWidthContainer">
    <div class="flowContent {{ options.maxWidth != 'full' ? ' centerContentMaxWidth'}}{{ options.maxWidth == 'large' ? 'Large' }}" {{ textAlign ? 'data-text-align="' ~ textAlign ~ '"' }}>
      {{ contentHtml|e('wp_kses_post') }}
    </div>
  </div>
</flynt-component>
