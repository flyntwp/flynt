<ul data-ref="menu" id="navigationBurger-menu" class="menu">
  {% for item in items %}
    <li class="item {{- item.current ? ' current-menu-item' }}">
      <a class="link" href="{{ item.link|e('esc_url') }}" {{ item.target == '_blank' ? 'target="_blank" rel="noopener noreferrer"' }}>{{ item.title }}</a>
    </li>
  {% endfor %}
</ul>
