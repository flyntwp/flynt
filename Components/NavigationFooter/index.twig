<flynt-component name="NavigationFooter" load:on="visible" data-theme="light">
  <div class="container centerContainer maxWidth--containerLarge">
    {% if menu %}
      <nav class="navigation" aria-label="{{ labels.ariaLabel }}">
        {% include 'Partials/_menu.twig' with { 'items': menu.get_items, 'level': '0', 'maxLevel': maxLevel } only %}
      </nav>
    {% endif %}
    {% if contentHtml %}
      <div class="content">{{ contentHtml|e('wp_kses_post') }}</div>
    {% endif %}
  </div>
</flynt-component>
