<flynt-component client:visible name="NavigationFooter" data-theme="dark">
  <div class="container centerMaxWidthContainerLarge">
    {% if menu %}
      <nav class="navigation" aria-label="{{ labels.ariaLabel }}">
        {% include 'Partials/_menu.twig' with { 'items': menu.get_items, 'level': '0', 'maxLevel': maxLevel } only %}
      </nav>
    {% endif %}
    {% if contentHtml %}
      <div class="flowContent">{{ contentHtml|e('wp_kses_post') }}</div>
    {% endif %}
  </div>
</flynt-component>
